    "@jest/types": "^27.1.1"
  },
  "scripts": {
    "b": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\"",
    "schema": "get-graphql-schema http://localhost:5702/graphql > schema/schema.graphql",
    "docs": "doctoc --title '**Table of content**' README.md",
    "clean": "lerna run clean",
    "build": "lerna run build",
    "pub": "lerna publish",
    "test": "lerna run test",
    "prettier": "prettier --write **/src/**/*.ts && prettier --write **/src/**/*.tsx",
    "app:relay": "yarn workspace @workshop/app relay --watchman false",
    "app:start": "yarn workspace @workshop/app start",
    "w": "yarn b webpackx.ts"
  },
