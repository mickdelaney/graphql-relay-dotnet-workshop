{
  "name": "workshop",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "resolutions": {
    "graphql": "15.4.0-experimental-stream-defer.1",
    "react": "0.0.0-experimental-f77c7b9d7",
    "react-dom": "0.0.0-experimental-f77c7b9d7",
    "styled-system": "5.1.5",
