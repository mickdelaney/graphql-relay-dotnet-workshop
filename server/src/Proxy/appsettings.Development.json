{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information"
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "Client": {
        "Destinations": {
          "Client1": {
            "Address": "https://localhost:5703"
          }
        }
      },
      "Server": {
        "Destinations": {
          "Server1": {
            "Address": "https://localhost:5702"
          }
        }
      }
    },
    "Routes": [
      {
        "RouteId": "ServerRoute",
        "ClusterId": "Server",
        "Match": {
          "Path": "/graphql/{**catch-all}"
        }
      },
      {
        "RouteId": "ServerSwaggerRoute",
        "ClusterId": "Server",
        "Match": {
          "Path": "/swagger/{**catch-all}"
        }
      },
      {
        "RouteId": "ClientRoute",
        "ClusterId": "Client",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    ]
  }
}